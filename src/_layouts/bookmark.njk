    <a class="u-bookmark-of h-cite" href="{{ 'bookmark-of' | getKeyedData | url }}" rel="external">
        <span class="h-cite">{{title}}</span>
    </a>
    {% set definedDate = date %}
    <div class="post-meta"><a href="{{url | url}}" class="u-url">ðŸ”—</a> | {% include "partials/date.njk" -%}</div>
    {% if content -%}
    <div class="e-content | flow prose">
      {{ content | safe }}
    </div>
    {% endif -%}
  </div>
</article>